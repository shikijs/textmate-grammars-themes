{
  "information_for_contributors": [""],
  "version": "1.2",
  "name": "Java Bytecode",
  "scopeName": "source.java.bytecode",
  "patterns": [
    { "include": "#patterns" }
  ],
  "repository": {
    "patterns": {
      "patterns": [
        {
          "match": "^\\s*\\b(static|public|private|protected)?\\s*[\\w$]+\\s*\\(.*\\)V",
          "name": "entity.name.function"
        },
        {
          "match": "^\\s*\\d+(:)",
          "name": "comment.code.label"
        },
        {
          "match": "//.*$",
          "name": "comment.line.double"
        },
        {
          "match": "/\\*.*?\\*/",
          "name": "comment.block"
        },
        {
          "match": "#.*$",
          "name": "comment.inline"
        },
        {
          "match": "\\b(aload|astore|aload_\\d|astore_\\d|getfield|getstatic|putfield|putstatic|invokeinterface|invokespecial|invokestatic|invokevirtual|invokedynamic|return|areturn|ireturn|dreturn|freturn|lreturn|nop|pop|pop2|dup|dup_x1|dup_x2|dup2|dup2_x1|dup2_x2|swap|iadd|isub|imul|idiv|irem|ineg|ishl|ishr|iushr|iand|ior|ixor|ladd|lsub|lmul|ldiv|lrem|lneg|lshl|lshr|lushr|land|lor|lxor|fadd|fsub|fmul|fdiv|frem|fneg|dadd|dsub|dmul|ddiv|drem|dneg|iinc|goto|goto_w|jsr|jsr_w|lookupswitch|tableswitch|wide|multianewarray|new|newarray|anewarray|arraylength|checkcast|instanceof|monitorenter|monitorexit)\\b",
          "name": "keyword.bytecode"
        },
        {
          "match": "\\b(aaload|baload|caload|daload|faload|iaload|laload|saload|aload(?:_\\d*)?)\\b",
          "name": "storage.load"
        },
        {
          "match": "\\b(aastore|bastore|castore|dastore|fastore|iastore|lastore|sastore|astore(?:_\\d*)?)\\b",
          "name": "storage.store"
        },
        {
          "match": "\\b(getfield|getstatic|putfield|putstatic)\\b",
          "name": "storage.modifier"
        },
        {
          "match": "\\b(invokeinterface|invokespecial|invokestatic|invokevirtual|invokedynamic)\\b",
          "name": "keyword.call"
        },
        {
          "match": "\\b(class|interface|abstract|static|public|private|protected|void|int|double|float|byte|char|short)\\b",
          "name": "keyword.java"
        },
        {
          "match": "\\d+",
          "name": "constant.numeric"
        },
        {
          "begin": "\"",
          "end": "\"",
          "name": "string.quoted.double",
          "patterns": [
            {
              "match": "\\\\.",
              "name": "constant.character.escape"
            }
          ]
        },
        {
          "match": "\\(.*\\)",
          "name": "variable.parameter.signature"
        }
      ]
    }
  },
  "uuid": "63357d86-fa78-4c49-867c-819f38185648"
}
