{
  "name": "tm-grammars",
  "type": "module",
  "version": "1.0.6",
  "packageManager": "pnpm@8.11.0",
  "description": "Collecton of TextMate grammars in JSON",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/textmate-grammars-themes#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/textmate-grammars-themes.git",
    "directory": "packages/tm-grammars"
  },
  "bugs": "https://github.com/antfu/textmate-grammars-themes/issues",
  "keywords": [
    "textmate"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    },
    "./grammars/*": "./grammars/*"
  },
  "main": "./index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./*",
        "./index.d.ts"
      ]
    }
  },
  "files": [
    "*.d.ts",
    "*.js",
    "LICENSE",
    "NOTICE",
    "grammars"
  ]
}
