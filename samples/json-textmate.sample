						},
						{
							"begin": "\\G(?={(?>[0-9]+\\\\{2}?,?[0-9]*|\\\\{2}?,?[0-9]+)\\\\{2}?})",
							"end": "\\\\{2}",
							"endCaptures": { "0": { "name": "constant.character.escape.tm" } },
							"contentName": "entity.name.label.tm strong regex"
