			]
		},
		"anchor": {
			"comment": "$ ^",
			"match": "[$^]([+*?]|{(?>[0-9]+,?[0-9]*|,[0-9]+)})?",
			"captures": {
				"0": { "name": "variable.tm" },
				"1": { "name": "invalid.illegal.tm regex" }
			}
		},
		"escaped-anchor": {
			"comment": "\\A \\B \\b \\G \\K \\Y \\y \\Z \\z",
			"match": "\\G\\\\{2}[ABbGKYyZz]([+*?]|{(?>[0-9]+,?[0-9]*|,[0-9]+)})?",
			"captures": {
				"0": { "name": "variable.tm" },
				"1": { "name": "invalid.illegal.tm regex" }
			}
		},
		"alternation": {
			"comment": "|",
			"match": "\\|([+*?]|{(?>[0-9]+,?[0-9]*|,[0-9]+)})?",
			"captures": {
				"0": { "name": "keyword.control.tm" },
				"1": { "name": "invalid.illegal.tm regex" }
