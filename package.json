{
  "name": "tm-grammars-themes",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@8.11.0",
  "description": "Collection of TextMate grammars and themes in JSON",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/tm-grammars-themes#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/tm-grammars-themes.git"
  },
  "bugs": "https://github.com/antfu/tm-grammars-themes/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm -r build",
    "stub": "pnpm -r stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.4.0",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.6",
    "@types/adm-zip": "^0.5.5",
    "@types/cson": "^7.20.3",
    "@types/js-yaml": "^4.0.9",
    "@types/json-stable-stringify": "^1.0.36",
    "@types/node": "^20.10.4",
    "adm-zip": "^0.5.10",
    "bumpp": "^9.2.0",
    "cson": "^8.2.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.55.0",
    "esno": "^4.0.0",
    "fast-plist": "^0.1.3",
    "js-yaml": "^4.1.0",
    "json-stable-stringify": "^1.1.0",
    "jsonc-parser": "^3.2.0",
    "lint-staged": "^15.2.0",
    "octokit": "^3.1.2",
    "ofetch": "^1.3.3",
    "p-limit": "^5.0.0",
    "pnpm": "^8.11.0",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vite": "^5.0.6",
    "vitest": "^1.0.2",
    "yaml": "^2.3.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
